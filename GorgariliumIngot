package periodicraft;

import net.minecraft.client.renderer.texture.IconRegister;
import net.minecraft.item.Item;

public class GorgarilumIngot extends Item {

  public GorgarilumIngot(int par1) {
		super(par1);
		
		this.setMaxStackSize(64);
		this.setCreativeTab(PeriodicraftMod.tabMetalistic);
		this.setUnlocalizedName("gorgariliumIngot");
	}

	@Override
	public void registerIcons(IconRegister reg){
		this.itemIcon = reg.registerIcon("periodicraft:gorgarilium_ingot");
	}
}
